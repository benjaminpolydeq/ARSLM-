{
  "version": 3,
  "name": "arslm-streamlit",
  "builds": [
    {
      "src": "apps/streamlit/requirements.txt",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12"
      }
    },
    {
      "src": "apps/streamlit/streamlit_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/streamlit/streamlit_app.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}